<?xml version="1.0" encoding="UTF-8"?>
<prov:document
  xmlns:prov="http://www.w3.org/ns/prov#"
  xmlns:rads="https://rads.igsp.duke.edu/"
  xmlns:dct="http://purl.org/dc/terms/"xmlns:foaf="http://xmlns.com/foaf/0.1/">
  <prov:entity prov:id="radsf:<%= @record.id %>">
    <dct:title><%= @record.content_file_name %></dct:title>
    <rads:md5><%= @record.content_fingerprint %></rads:md5>
  </prov:entity>
<% @record.audited_activities.each do |aa| %>
<%= render partial: "audited_activity", object: aa %>
<% end %>
</prov:document>
